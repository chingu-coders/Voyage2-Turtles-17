{
    "manifest_version": 2,
  
    "name": "Momentum clone",
    "description": "Momentum clone with weather, links, to-do tabs",
    "version": "0.1",
  
    "browser_action": {
      "default_icon": "icons/16.png",
      "default_title": "Momentum clone"
      
    },
    "icons": 
	{ 
		"16": 				          "icons/16.png",
		"48": 				          "icons/48.png",
		"128": 				          "icons/128.png"
	},
    "permissions": [
      "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
      }
  }